{
  "name": "rbac-system",
  "version": "1.0.0",
  "description": "Comprehensive RBAC System with Audit Logging",
  "main": "index.js",
  "scripts": {
    "dev:backend": "nodemon --exec ts-node src/backend/server.ts",
    "dev:frontend": "vite",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "node dist/backend/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node src/backend/prisma/seed.ts"
  },
  "keywords": ["rbac", "roles", "permissions", "audit-log"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
